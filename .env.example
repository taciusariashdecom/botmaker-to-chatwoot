# Copy to .env and fill in values. Do NOT commit .env

# Botmaker
BOTMAKER_BASE_URL=https://api.botmaker.com/v2.0
BOTMAKER_API_TOKEN=
BOTMAKER_BUSINESS_ID=
BOTMAKER_USER_ID=

# Chatwoot (Application API)
CHATWOOT_BASE_URL=https://app.chatwoot.com
CHATWOOT_API_ACCESS_TOKEN=
CHATWOOT_ACCOUNT_ID=
CHATWOOT_INBOX_ID=

# Chatwoot (Client API / API channel)
CHATWOOT_INBOX_IDENTIFIER=
CHATWOOT_HMAC_TOKEN=

# Storage backend: local | supabase
STORAGE_BACKEND=local

# Local storage paths (used if STORAGE_BACKEND=local)
DATA_DIR=/app/data
LOG_DIR=/app/logs
MAPPINGS_DIR=/app/mappings

# Supabase (used if STORAGE_BACKEND=supabase)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_BUCKET=botmaker-raw
SUPABASE_SCHEMA=public
SUPABASE_DB_URL=

# Tuning
RATE_LIMIT_RPS=4
PAGE_SIZE=100
CHUNK_SIZE=200

# Extraction window (ISO-8601)
EXTRACT_START=
EXTRACT_END=
